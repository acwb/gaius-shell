gmsg Gaius -notice "Dossier Ã  traiter ?"
echo -n " "
read pathdoublons
if [ -n "$pathdoublons" ]; then
    cd $pathdoublons
    find -not -empty -type f -printf "%s\n" | sort -rn | uniq -d | xargs -I{} -n1 find -type f -size {}c -print0 | xargs -0 md5sum | sort | uniq -w32 --all-repeated=separate
fi